{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "front/build",
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/[^.]+",
      "dest": "front/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
